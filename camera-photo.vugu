<div>
	<button @click='c.Navigate("/camera/" + c.camera.Key(), nil)'>Back</button>
	<h1>Photo <span vg-content="c.Key()"></span></h1>

	<main:CoordinateComponent :Editable="true" :BindValue="&c.Position"></main:CoordinateComponent>
	<main:RotationComponent :Editable="true" :BindValue="&c.Orientation"></main:RotationComponent>

	<main:CameraPhotoComponent :Photo="c"></main:CameraPhotoComponent>

</div>
