<div vg-attr="c.AttrMap">
	<vg-template vg-if="c.BindValue != nil && c.Editable">
		<main:DistanceInputComponent :BindValue="&c.BindValue.Coordinate[0]" :BindLock="&c.BindValue.Locked[0]" LabelText="X"></main:DistanceInputComponent>
		<main:DistanceInputComponent :BindValue="&c.BindValue.Coordinate[1]" :BindLock="&c.BindValue.Locked[1]" LabelText="Y"></main:DistanceInputComponent>
		<main:DistanceInputComponent :BindValue="&c.BindValue.Coordinate[2]" :BindLock="&c.BindValue.Locked[2]" LabelText="Z"></main:DistanceInputComponent>
	</vg-template>
	<vg-template vg-if="c.BindValue != nil && !c.Editable">
		<div>X: </span><span vg-content='fmt.Sprintf("%.4f", c.BindValue.Coordinate.X())'></div>
		<div>Y: </span><span vg-content='fmt.Sprintf("%.4f", c.BindValue.Coordinate.Y())'></div>
		<div>Z: </span><span vg-content='fmt.Sprintf("%.4f", c.BindValue.Coordinate.Z())'></div>
	</vg-template>
</div>

<style>
</style>
