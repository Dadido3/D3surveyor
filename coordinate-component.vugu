<div vg-attr="c.AttrMap">
	<vg-template vg-if="c.BindValue != nil && c.Editable">
		<main:DistanceInputComponent :BindValue="&c.BindValue[0]" LabelText="X"></main:DistanceInputComponent>
		<main:DistanceInputComponent :BindValue="&c.BindValue[1]" LabelText="Y"></main:DistanceInputComponent>
		<main:DistanceInputComponent :BindValue="&c.BindValue[2]" LabelText="Z"></main:DistanceInputComponent>
	</vg-template>
	<vg-template vg-if="c.BindValue != nil && !c.Editable">
		<div>X: </span><span vg-content='fmt.Sprintf("%.4f", c.BindValue.X())'></div>
		<div>Y: </span><span vg-content='fmt.Sprintf("%.4f", c.BindValue.Y())'></div>
		<div>Z: </span><span vg-content='fmt.Sprintf("%.4f", c.BindValue.Z())'></div>
	</vg-template>
</div>

<style>
</style>
